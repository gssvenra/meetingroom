{
  "type": "object",
  "properties": {
    "date": {
      "type": "date",
      "selector": "#startDate",
      "control": "text",
      "format": "MMM DD, YYYY"
    },
    "time": {
      "type": "time",
      "selector": "#startTime",
      "control": "text",
      "format": "HH:mm"
    },
    "duration": {
      "type": "string",
      "selector": "#duration",
      "control": "select"
    },
    "ishome": {
      "type": "identifier",
      "selector": ".col-sm-12.heading.margintop10",
      "control": "exists"
    },
    "floor": {
      "type": "select",
      "selector": ".select.ng-pristine.ng-valid.ng-not-empty.ng-touched",
      "control": "select",
      "selectContains": "All",
      "selectIndex": 1
    }
  },
  "connect": [
    {
      "match": [
        "ishome"
      ],
      "screens": [
        "home"
      ]
    }
  ]
}
